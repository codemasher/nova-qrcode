{
	"name": "codemasher/nova-qrcode",
	"description": "A QR code generator. PHP 7.4+",
	"homepage": "https://github.com/codemasher/nova-qrcode",
	"license": "MIT",
	"minimum-stability": "stable",
	"require": {
		"php": "^7.4 || ^8.0",
		"ext-json": "*",
		"chillerlan/php-qrcode": "^5.0",
		"simple-icons/simple-icons": "^11"
	},
	"autoload": {
		"psr-4": {
			"codemasher\\NovaQRCode\\": "src/"
		}
	},
	"config": {
		"lock": false,
		"sort-packages": true,
		"platform-check": true
	}
}
